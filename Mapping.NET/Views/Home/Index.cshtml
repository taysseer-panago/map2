                map: map,
                bounds: {
                    north: 49.382483,
                    south: 49.372620,
                    east: -123.264453,
                    west: -123.280784
                }
            });
            //east-west difference: .016331
            //every square towards the east, add east-west difference
            //north-south difference: .009863
            //every square towards the south, subtract north-south difference

            
        }
            </script>
