            //change 1
            //place marker
            var marker = new google.maps.Marker({ position: myCenter });
            marker.setMap(map);

            var rectangleCol[];
            var north = 49.382483;
            var south = 49.372620;
            var east = -123.280784;
            var west = -123.297115;

            for (ns = 1; ns++; ns < 35) {                
                var thisNorth = north - (ns * 0.009863);
                var thisSouth = south - (ns * 0.009863);
                for (ew = 0; ew++; ew < 51) {
                    var thisEast = east + (ew * 0.016331);
                    var thisWest = west + (ew * 0.016331);
                    console.log(thisNorth, thisSouth, thisEast, thisWest);
                    rectangleCol.push({ north: thisNorth, south: thisSouth, east: thisEast, west: thisWest });
                }
            }

            //place square
            var rectangle1 = new google.maps.Rectangle({
                strokeColor: '#FF0000',
                strokeOpacity: 0.8,
                strokeWeight: 2,
                fillColor: '#FF0000',
                fillOpacity: 0.35,
                map: map,
                bounds: {
                    north: 49.382483,
                    south: 49.372620,
                    east: -123.280784,
                    west: -123.297115
                }
            });
            //east-west difference: .016331
            //every square towards the east, add east-west difference
            //north-south difference: .009863
